{
  "setupTasks": [
    {
      "name": "Install Dependencies",
      "command": "pnpm add pnpm && pnpm i --force"
    }
  ],
  "tasks": {
    "start": {
      "name": "start",
      "command": "pnpm run start",
      "runAtStart": false
    },
    "dev": {
      "name": "dev",
      "command": "pnpm run dev",
      "runAtStart": true,
      "preview": {
        "port": 3000
      }
    },
    "build": {
      "name": "build",
      "command": "pnpm run build",
      "runAtStart": false
    },
    "serve": {
      "name": "serve",
      "command": "pnpm run serve",
      "runAtStart": false
    },
    "test": {
      "name": "test",
      "command": "pnpm run test",
      "runAtStart": false
    },
    "test:watch": {
      "name": "test:watch",
      "command": "pnpm run test:watch",
      "runAtStart": false
    },
    "lint": {
      "name": "lint",
      "command": "pnpm run lint",
      "runAtStart": false
    },
    "lint:fix": {
      "name": "lint:fix",
      "command": "pnpm run lint:fix",
      "runAtStart": false
    },
    "typecheck": {
      "name": "typecheck",
      "command": "pnpm run typecheck",
      "runAtStart": false
    },
    "prettier": {
      "name": "prettier",
      "command": "pnpm run prettier",
      "runAtStart": false
    },
    "clean": {
      "name": "clean",
      "command": "pnpm run clean",
      "runAtStart": false
    },
    "validate": {
      "name": "validate",
      "command": "pnpm run validate",
      "runAtStart": false
    },
    "remove:tailwind": {
      "name": "remove:tailwind",
      "command": "pnpm run remove:tailwind",
      "runAtStart": false
    }
  }
}
